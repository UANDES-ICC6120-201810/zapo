<p id="notice"><%= notice %></p>

<h1>File To Updates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>File</th>
      <th>File package</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_to_updates.each do |file_to_update| %>
      <tr>
        <td><%= file_to_update.name %></td>
        <td><%= file_to_update.file %></td>
        <td><%= file_to_update.file_package_id %></td>
        <td><%= link_to 'Show', file_to_update %></td>
        <td><%= link_to 'Edit', edit_file_to_update_path(file_to_update) %></td>
        <td><%= link_to 'Destroy', file_to_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File To Update', new_file_to_update_path %>
